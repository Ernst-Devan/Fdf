{
  "version": "0.2.0",
  "configurations": [
	{
		"type": "lldb",
		"request": "launch",
		"name": "(lldb) Launch",
		"program": "${workspaceFolder}/fdf.out",
		"args": [],
		"cwd": "${workspaceFolder}",
		"preLaunchTask": "build"
	},
	{
		"name": "(gdb) Launch",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/fdf.out",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${fileDirname}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"preLaunchTask": "build",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		]
	}
  ]
}